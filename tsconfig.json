{
	"ts-node": {
		"transpileOnly": true,
		"transpiler": "ts-node/transpilers/swc-experimental",
		"files": true
	},

	"compilerOptions": {
		"lib": ["ESNext", "DOM"],
		"module": "CommonJS",
		"target": "ESNext",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true
	}
}
